   "license": "UNLICENSED",
   "dependencies": {
      "@zeit/next-typescript": "^1.1.0",
      "babel-plugin-styled-components": "^1.6.4",
      "bee-queue": "^1.2.2",
      "cross-env": "^5.2.0",
      "date-fns": "^1.29.0",
      "mysql": "^2.14.1",
      "mysql2": "^1.5.3",
      "next": "^6.1.2",
      "npm-run-all": "^4.1.3",
      "pm2": "^2.10.4",
      "progress": "^2.0.0",
      "react": "^16.4.0",
      "react-dom": "^16.4.0",
      "reflect-metadata": "^0.1.10",
      "scrape-it": "^5.0.5",
      "typeorm": "0.2.7",
      "typescript": "^3.0.3"
   },
   "scripts": {
      "dev": "nodemon -r dotenv/config server/index.ts",
      "build": "next build",
      "build-server": "tsc --project tsconfig.server.json",
      "build-scraper": "tsc --project tsconfig.scraper.json",
      "serve": "cross-env IS_SERVER=1 node .next/production-server/server/index.js",
      "scrape-categories-dev": "ts-node scraper/scrape-categories.ts",
      "scrape-categories": "cross-env IS_SCRAPER=1 node .scraper-dist/scraper/scrape-categories.js",
      "process-dev": "ts-node scraper/process.ts",
      "process": "cross-env IS_SCRAPER=1 node .scraper-dist/scraper/process.js",
      "synchronize": "typeorm schema:sync",
      "prestart": "run-s synchronize build build-server build-scraper",
      "start": "pm2 start process.yml -i max",
      "poststart": "pm2 logs"
   },
   "devDependencies": {
      "@types/cheerio": "^0.22.9",
      "@types/next": "^6.1.4",
      "@types/node": "^8.0.29",
      "@types/react": "^16.4.14",
      "dotenv": "^6.0.0",
      "nodemon": "^1.18.4",
      "ts-node": "3.3.0"
   }
}
